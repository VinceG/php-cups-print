{
    "name": "vinceg/php-cups-print",
    "type": "library",
    "description": "PHP Wrapper around Mac CUPS printing system",
    "keywords": [
        "php",
        "cups",
        "printing"
    ],
    "homepage": "https://github.com/vinceg/php-cups-print",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Gabriel",
            "email": "vadimg88@gmail.com",
            "homepage": "https://github.com/vinceg",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "tightenco/collect": "^6.9"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Printing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Printing\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
